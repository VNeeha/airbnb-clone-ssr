<%- include('partials/head') %>
</head>
<body class="bg-white ">
    <%- include('partials/nav')%>

    <main class="container mx-auto shadow-lg rounded-lg bg-white p-8 mt-10 max-w-xl">
        <h2 class="text-3xl text-red-500 font-bold text-center mb-6">Here are our registered homes:</h2>
        <ul class="space-y-4">
            <%registeredHomes.forEach((home)=>{%>
                <li class="bg-gray-100 p-3 rounded-lg shadow-md hover:shadow-lg transition duration-300">
                   <span class="text-lg text-gray-800"> <%=home.houseName%>
                   </span>
                </li>
            <%});%>
        </ul>
    </main>
    
</body>
</html>